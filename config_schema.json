{
  "title": "ModSettings",
  "description": "Modifies the orginal Settings class provided by the user",
  "type": "object",
  "properties": {
    "s3_endpoint_url": {
      "title": "S3 Endpoint Url",
      "env_names": [
        "upload_controller_service_s3_endpoint_url"
      ],
      "type": "string"
    },
    "s3_access_key_id": {
      "title": "S3 Access Key Id",
      "env_names": [
        "upload_controller_service_s3_access_key_id"
      ],
      "type": "string"
    },
    "s3_secret_access_key": {
      "title": "S3 Secret Access Key",
      "env_names": [
        "upload_controller_service_s3_secret_access_key"
      ],
      "type": "string"
    },
    "s3_session_token": {
      "title": "S3 Session Token",
      "env_names": [
        "upload_controller_service_s3_session_token"
      ],
      "type": "string"
    },
    "aws_config_ini": {
      "title": "Aws Config Ini",
      "env_names": [
        "upload_controller_service_aws_config_ini"
      ],
      "type": "string",
      "format": "path"
    },
    "db_url": {
      "title": "Db Url",
      "env_names": [
        "upload_controller_service_db_url"
      ],
      "type": "string"
    },
    "db_print_logs": {
      "title": "Db Print Logs",
      "default": false,
      "env_names": [
        "upload_controller_service_db_print_logs"
      ],
      "type": "boolean"
    },
    "service_name": {
      "title": "Service Name",
      "default": "upload_controller_service",
      "env_names": [
        "upload_controller_service_service_name"
      ],
      "type": "string"
    },
    "rabbitmq_host": {
      "title": "Rabbitmq Host",
      "default": "rabbitmq",
      "env_names": [
        "upload_controller_service_rabbitmq_host"
      ],
      "type": "string"
    },
    "rabbitmq_port": {
      "title": "Rabbitmq Port",
      "default": 5672,
      "env_names": [
        "upload_controller_service_rabbitmq_port"
      ],
      "type": "integer"
    },
    "host": {
      "title": "Host",
      "default": "127.0.0.1",
      "env_names": [
        "upload_controller_service_host"
      ],
      "type": "string"
    },
    "port": {
      "title": "Port",
      "default": 8080,
      "env_names": [
        "upload_controller_service_port"
      ],
      "type": "integer"
    },
    "log_level": {
      "title": "Log Level",
      "default": "info",
      "env_names": [
        "upload_controller_service_log_level"
      ],
      "enum": [
        "critical",
        "error",
        "warning",
        "info",
        "debug",
        "trace"
      ],
      "type": "string"
    },
    "auto_reload": {
      "title": "Auto Reload",
      "default": false,
      "env_names": [
        "upload_controller_service_auto_reload"
      ],
      "type": "boolean"
    },
    "workers": {
      "title": "Workers",
      "default": 1,
      "env_names": [
        "upload_controller_service_workers"
      ],
      "type": "integer"
    },
    "api_root_path": {
      "title": "Api Root Path",
      "default": "/",
      "env_names": [
        "upload_controller_service_api_root_path"
      ],
      "type": "string"
    },
    "openapi_url": {
      "title": "Openapi Url",
      "default": "/openapi.json",
      "env_names": [
        "upload_controller_service_openapi_url"
      ],
      "type": "string"
    },
    "docs_url": {
      "title": "Docs Url",
      "default": "/docs",
      "env_names": [
        "upload_controller_service_docs_url"
      ],
      "type": "string"
    },
    "cors_allowed_origins": {
      "title": "Cors Allowed Origins",
      "env_names": [
        "upload_controller_service_cors_allowed_origins"
      ],
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "cors_allow_credentials": {
      "title": "Cors Allow Credentials",
      "env_names": [
        "upload_controller_service_cors_allow_credentials"
      ],
      "type": "boolean"
    },
    "cors_allowed_methods": {
      "title": "Cors Allowed Methods",
      "env_names": [
        "upload_controller_service_cors_allowed_methods"
      ],
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "cors_allowed_headers": {
      "title": "Cors Allowed Headers",
      "env_names": [
        "upload_controller_service_cors_allowed_headers"
      ],
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "s3_inbox_bucket_id": {
      "title": "S3 Inbox Bucket Id",
      "default": "inbox",
      "env_names": [
        "upload_controller_service_s3_inbox_bucket_id"
      ],
      "type": "string"
    },
    "topic_name_upload_received": {
      "title": "Topic Name Upload Received",
      "default": "file_upload_received",
      "env_names": [
        "upload_controller_service_topic_name_upload_received"
      ],
      "type": "string"
    },
    "topic_name_new_study": {
      "title": "Topic Name New Study",
      "default": "new_study_created",
      "env_names": [
        "upload_controller_service_topic_name_new_study"
      ],
      "type": "string"
    }
  },
  "required": [
    "s3_endpoint_url",
    "s3_access_key_id",
    "s3_secret_access_key",
    "db_url"
  ],
  "additionalProperties": false
}
